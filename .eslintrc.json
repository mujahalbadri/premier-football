{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-globals": "off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "func-names": "off",
        "no-param-reassign": "off",
        "no-shadow": "off",
        "no-unused-vars": "off",
        "indent": "off",
        "no-plusplus": "off",
        "no-mixed-operators": "off",
        "no-alert": "off"
    }
}